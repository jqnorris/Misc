#include <iostream>
#include "Framework.h"
#include "SimulationFunctions.h"
#include "TestingSimulationFunctions.h"

using std::cout;
using std::endl;

int main()
{
	Options options;

	options.v0 = 1;
	options.N=5;
	options.latticeType=4;
	options.E0=0;
	options.epsilonY0=0;
	options.k0=0;
	options.phi0=0;

	PoreArray pores(options);

	Matrix matrix(options);

	setupPoreNeighbors(pores, matrix, options);
	setupMatrixNeighbors(pores, matrix, options);

	cout << endl
	for(int i=0; i<10; i++)
	{
		int a=4*randKnuth();
		cout << a << " ";
	}

	testDamageMatrix(pores,options);


	return 0;
}


