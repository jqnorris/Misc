{
	if(options.latticeType==4)
	{
		for(int i=0; i<pores.dimension; i++)
		{
			for(int j=0; j<pores.dimension; j++)
			{
				pores.array[i][j].neighbors = new * Rock[options.latticType];

				if(i==0)
				{

				}
			}
		}
	}
}

